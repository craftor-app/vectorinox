{
  "name": "vectorinox",
  "version": "0.0.6",
  "description": "Cleans up and compresses SVG files exported from Sketch (and other design tools). Optionally formats SVG for inlining into React projects.",
  "author": "Stefano J. Attardi (http://github.com/steadicat)",
  "homepage": "https://github.com/steadicat/vectorinox",
  "repository": {
    "type": "git",
    "url": "git@github.com:steadicat/vectorinox.git"
  },
  "bugs": {
    "url": "https://github.com/steadicat/vectorinox/issues"
  },
  "keywords": [
    "svg",
    "vector",
    "image",
    "graphics",
    "optimizer",
    "minifier",
    "compress",
    "minify",
    "optimize",
    "jsx",
    "react",
    "format",
    "sketch",
    "export"
  ],
  "bin": {
    "vectorinox": "./dist/bin.js"
  },
  "scripts": {
    "build": "./node_modules/.bin/tsc",
    "prepublish": "npm run build",
    "test": "./node_modules/.bin/mocha -r ts-node/register test/**/*.ts",
    "test:watch": "./node_modules/.bin/mocha --watch --watch-extensions ts -r ts-node/register test/**/*.ts src/plugins/*.ts"
  },
  "dependencies": {
    "chai": "^4.1.2",
    "d-path-parser": "^1.0.0",
    "ltx": "^2.7.1",
    "yargs": "^10.0.3"
  },
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/ltx": "^2.6.2",
    "@types/mocha": "^2.2.44",
    "@types/yargs": "^8.0.2",
    "expect": "^21.2.1",
    "mocha": "^4.0.1",
    "ts-node": "^3.3.0",
    "typescript": "^2.5.3"
  },
  "license": "MIT"
}
